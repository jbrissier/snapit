{%extends '__base.haml' %}
{% load staticfiles %}


-block 'content'
    %ul
        - for ev in object_list
            %li
                %a{href:"{{ev.get_absolute_url}}"}
                    Upload {{ev.name}} {{ev.uuid}}

                %a{href:"{% url 'snapid_view' ev.get_eventuuid%}"}
                    View {{ev.name}} {{ev.uuid}}